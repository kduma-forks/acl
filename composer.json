{
    "name": "renoki-co/acl",
    "description": "Simple ACL for PHP applications, AWS IAM style.",
    "keywords": ["laravel", "php"],
    "license": "Apache-2.0",
    "homepage": "https://github.com/renoki-co/acl",
    "authors": [
        {
            "name": "Alex Renoki",
            "homepage": "https://github.com/rennokki",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^9.41"
    },
    "autoload": {
        "psr-4": {
            "RenokiCo\\Acl\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RenokiCo\\Acl\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0",
        "vimeo/psalm": "4.24.0"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
